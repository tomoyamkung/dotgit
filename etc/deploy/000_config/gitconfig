[user]
    name = _NAME_
    email = _EMAIL_
[core]
    editor = _EDITOR_
[color]
    ui = true
[alias]
    br = branch
    ch = checkout
    ci = commit
    di = diff
    log = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    loga = log --graph --all --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    st = status -sbu

